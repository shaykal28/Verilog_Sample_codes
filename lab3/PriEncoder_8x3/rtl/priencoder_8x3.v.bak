module priencoder_8x3(input [7:0] data_in,
		      input enable,
		      output reg [2:0] data_out);

 always@(data_in,enable) begin
   if(enable==1'b1) begin
        if(data_in[7]==1'b1)
	   data_out = 3'b111;
	else if(data_in[6]==1'b1)
	   data_out = 3'b110; 
        else if(data_in[5]==1'b1)
	   data_out = 3'b101;
        else if(data_in[4]==1'b1)
	   data_out = 3'b100;
        else if(data_in[3]==1'b1)
	   data_out = 3'b011;
        else if(data_in[2]==1'b1)
	   data_out = 3'b010;
        else if(data_in[1]==1'b1)
	   data_out = 3'b001;
        else if(data_in[0]==1'b1)
	   data_out = 3'b000;
	else data_out = 3'bzzz; 
        data_out = 3'bzzz;
   end
 
   else begin
   data_out = 3'bzzz; 
   end

 end

endmodule
 	
